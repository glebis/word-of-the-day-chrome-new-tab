{
  "name": "Word of the Day",
  "description": "Override the new tab page with Merriam Webster Word of the Day",
  "version": "0.1",
  "incognito": "split",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "wod.html"
  },

  "manifest_version": 2,
  "permissions": ["http://*/","http://*/*"]
}
